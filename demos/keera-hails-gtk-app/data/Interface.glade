<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="mainWindow">
    <property name="width_request">800</property>
    <property name="height_request">600</property>
    <property name="can_focus">False</property>
    <child>
      <object class="GtkFixed" id="mainWindowFxd">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <child>
          <object class="GtkLabel" id="mainWindowMessageLbl">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">Hello World!!!</property>
          </object>
          <packing>
            <property name="x">13</property>
            <property name="y">8</property>
          </packing>
        </child>
        <child>
          <object class="GtkDrawingArea" id="mainDrawingArea">
            <property name="width_request">767</property>
            <property name="height_request">539</property>
            <property name="visible">True</property>
            <property name="can_focus">False</property>
          </object>
          <packing>
            <property name="x">17</property>
            <property name="y">49</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
